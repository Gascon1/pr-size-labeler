{
  "extends": ["plugin:github/recommended", "@bedrockstreaming", "@bedrockstreaming/eslint-config/typescript"],
  "plugins": ["jest"],
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "project": "./tsconfig.eslint.json"
  },
  "rules": {
    "filenames/match-regex": "off",
    "i18n-text/no-en": "off"
  },
  "env": {
    "node": true,
    "es6": true,
    "jest/globals": true
  }
}
